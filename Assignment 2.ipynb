    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Input word:ineuron\n",
      "Output: norueni\n"
     ]
    }
   ],
   "source": [
    "i = input(\"Input word:\")\n",
    "print(\"Output:\",i[::-1])"
   ]
  }
